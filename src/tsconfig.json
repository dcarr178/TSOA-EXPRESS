{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "outDir": "../dist",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "api",
    "webServer/api"
  ]
}